<launch>
    <!-- LIDAR -->
    <include file="$(find-pkg-share rplidar_ros)/launch/rplidar_a1_launch.py">
        <arg name="serial_port" value="/dev/lidar"/>
        <arg name="frame_id" value="base_scan"/>
        <arg name="angle_compansate" value="True"/>
        <arg name="serial_baudrate" value="115200"/>
    </include>

    <!-- IMU -->
    <include file="$(find-pkg-share bno055)/launch/bno055.launch.py"/>
</launch>